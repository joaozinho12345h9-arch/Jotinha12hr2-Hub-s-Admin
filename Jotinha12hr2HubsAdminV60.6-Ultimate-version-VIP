--[[ 
    JHUB CLIENTE ADMIN V60.4 - MOBILE OPTIMIZED
    - SUPORTADO: Delta / Arceus X Neo / Codex
    - PROTEÇÃO: Byte-Layer 256
]]

-- Link codificado para não ser detectado por filtros de log do executor
local _0x_DATA = {104,116,116,112,115,58,47,47,114,97,119,46,103,105,116,104,117,98,117,115,101,114,99,111,110,116,101,110,116,46,99,111,109,47,106,111,97,111,122,105,110,104,111,49,50,51,52,53,104,57,45,97,114,99,104,47,74,111,116,105,110,104,97,49,50,104,114,50,45,72,117,98,45,115,45,65,100,109,105,110,47,114,101,102,115,47,104,101,97,100,115,47,109,97,105,110,47,74,111,116,105,110,104,97,49,50,104,114,50,72,117,98,115,65,100,109,105,110,86,54,48,46,52,45,85,108,116,109,97,116,101,45,118,101,114,115,105,111,110,45,86,73,80}

local function _0x_FETCH()
    local _0x_URL = ""
    for _, _0x_B in ipairs(_0x_DATA) do
        _0x_URL = _0x_URL .. string.char(_0x_B)
    end
    return _0x_URL
end

-- Sistema de Carregamento para Mobile (Arceus/Delta)
local function _0x_START()
    local _0x_SUCCESS, _0x_ERROR = pcall(function()
        local _0x_RAW = game:HttpGet(_0x_FETCH())
        if _0x_RAW and #_0x_RAW > 0 then
            loadstring(_0x_RAW)()
        else
            error("Falha no Resgate de Dados")
        end
    end)

    if not _0x_SUCCESS then
        -- Se falhar no Delta, tenta novamente em 2 segundos
        warn("JHUB: Tentando reconectar ao servidor...")
        task.wait(2)
        _0x_START()
    else
        -- Notificação dentro do jogo usando o sistema nativo do Roblox
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "JHUB CLIENTE ADMIN",
            Text = "Sistema VIP Ativado com Sucesso!",
            Icon = "rbxassetid://4483362458",
            Duration = 5
        })
    end
end

-- Inicia o processo em uma thread separada para não travar o Arceus X
task.spawn(_0x_START)

